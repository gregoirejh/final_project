doctype html
html(lang="en")
head
    title AST - Logs
    link(rel='stylesheet', href='/css/bootstrap.css')
    script(type="text/javascript" src="/js/jquery-3.2.1.min.js" charset="utf-8")
    script(type="text/javascript" src="/js/bootstrap.min.js" charset="utf-8")
    script(type="text/javascript" src='/socket.io/socket.io.js' charset="utf-8")
body
    .container
    h1 Logs
    #content
script 
    :coffee-script
        socket = io.connect 'http://localhost:8888'
        socket.on 'logs', (data) ->
            $('#content').append "<p style='margin:0'>#{data.username} loaded #{data.url}</p>"